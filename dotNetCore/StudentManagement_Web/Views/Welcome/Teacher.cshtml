@{
    Layout = "_WelcomeLayout";
    ViewData["Title"] = "老师";
    ViewData["JsFunction"] = "TeacherFunction.js";
}
<div>
    <nav>
        <div class="nav nav-tabs" id="nav-tab" role="tablist">
            <a class="nav-item nav-link active" id="nav-TeacherView-tab" data-toggle="tab" href="#nav-TeacherView" role="tab" aria-controls="nav-TeacherView" aria-selected="true" onclick="onclickTeacherTab('addCourse')">增加课程</a>
            <a class="nav-item nav-link" id="nav-TeacherView-tab" data-toggle="tab" href="#nav-TeacherView" role="tab" aria-controls="nav-TeacherView" aria-selected="false" onclick="onclickTeacherTab('applyExam')">申请考试</a>
